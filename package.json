{
  "private": true,
  "scripts": {
    "build-debugger": "cross-env NODE_ENV=dev babel-node ./webpack/bundler.js --trace-deprecation",
    "build": "cross-env NODE_ENV=dev babel-node ./webpack/bundler.js",
    "prod": "cross-env NODE_ENV=prod babel-node ./webpack/bundler.js",
    "start": "npm prune && npm i",
    "watch": "cross-env NODE_ENV=dev WATCH=true babel-node ./webpack/bundler.js",
    "ie": "babel --plugins transform-es2015-classes public/javascript/*.js --out-file public/javascript/*.js && minify public/javascript -d public/javascript",
    "critical": "cross-env NODE_ENV=prod CRITICAL=true babel-node ./webpack/critical-css.js",
    "imgmin": "cross-env NODE_ENV=prod babel-node ./webpack/imgmin.js"
  },
