    "babel-core": "^6.26.3",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-minify": "^0.4.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
