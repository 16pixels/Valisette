{
  "private": true,
  "scripts": {
    "build-debugger": "cross-env NODE_ENV=development babel-node ./webpack/bundler.js --trace-deprecation",
    "build": "cross-env NODE_ENV=development babel-node ./webpack/bundler.js",
    "prod": "cross-env NODE_ENV=production babel-node ./webpack/bundler.js",
    "watch": "cross-env NODE_ENV=development WATCH=true babel-node ./webpack/bundler.js",
    "imgmin": "cross-env NODE_ENV=production babel-node ./webpack/imgmin.js",
    "conf": "babel-node ./webpack/generate-conf.js",
    "generate": "babel-node ./webpack/make-boilerplate.js",
    "reset": "npm run conf && npm run generate && npm run imgmin",
    "clean-node": "rm -rf node_modules && npm i",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:publish": "./deploy.sh",
    "test": "cross-env NODE_ENV=none jest ./tests/ --notify --detectOpenHandles",
    "wt": "cross-env NODE_ENV=none jest ./tests/ --notify --watchAll --detectOpenHandles"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    }
  },
